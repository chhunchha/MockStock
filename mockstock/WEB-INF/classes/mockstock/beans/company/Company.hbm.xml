<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="mockstock.beans.company.Company" table="company">
    <id name="companyid"/>
    <property name="name"/>
    <property name="price"/>
    <property name="lot"/>
  </class>
</hibernate-mapping>
